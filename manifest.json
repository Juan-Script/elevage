{
    "name": "Elevage",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
      "default_popup": null
    },
    "icons": {
      "128": "logo.png"
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  